{
  "name": "Schmup Wars",
  "description": "There Goes The Planer...",
  "version": "0.6",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "notifications",
    "<all_urls>"
  ],
  "requirements": {
    "3D": {
      "features": ["webgl"]
    }
  },
  "icons": { "16": "res/favicon.png", "128": "res/favicon128.png" }
}